<section class="content-header">
	<div class="row">
		<div class="col-md-8">
			<h3 style="margin-top: 5px">
				{% block pageName %}{% endblock %}
			</h3>
		</div>
		<div class="col-md-4">
			<div class="pull-right">
			{% block actionButtons %}{% endblock %}
			</div>
		</div>
	</div>
</section>
	<div class="col-md-12" ng-controller="AlertController">
	{% verbatim %}
		<div ng-repeat="alertItem in alerts" class="alert alert-dismissable alert-{{ level }}" ng-class="">
			<button type="button" class="close" aria-hidden="true" ng-click="dismissAlert(alertItem)">Ã—</button>
			{{ alertItem.message }}
		</div>
	{% endverbatim %}
	</div>
<section class="content">
<div class="row">
    <div class="col-md-12">
    	<div class="box box-primary" style='padding-top:15px'>
    		<div class="box-body" >
    			{% block pageContent %}{% endblock %}
    		</div>
    	</div> 
    </div>
</div>
</section>